<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.capstone_1.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_emergencycontact"
        android:name="com.example.capstone_1.ui.Emergency_Contact.EmergencyFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_emergency" />

    <fragment
        android:id="@+id/navigation_reportForm"
        android:name="com.example.capstone_1.ui.history_form.HistoryFragment"
        android:label="Riwayat Laporan"
        tools:layout="@layout/fragment_history_report" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.capstone_1.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile"/>

</navigation>